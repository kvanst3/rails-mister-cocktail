 <div class="container">
  <h1>My Cocktails</h1>
  <p style="text-align: right"> <%= link_to "+ Add Cocktail", new_cocktail_path(@cocktail)  %> </p>
  <% @cocktails.each do |cocktail| %>
    <ul style="list-style: none">
      <li> <%= link_to cocktail.name, cocktail_path(cocktail) %> </li>
    </ul>
  <% end %>
</div>
